all: ${OBJ} libX11.a

libX11.a: ${OBJ}
	touch libX11.a
	rm libX11.a
	cd ../Xau;make
	cd ../SM;make
	cd ../ICE;make
	cd ../Xtst;make
	cd ../xkbfile;make
	cd ../Xi;make
	cd ../Xdmcp;make
	cd ../Xext;make
	cd ../XIE;make
	${AR} libX11.a ${OBJ} $(AuOBJ) $(SMOBJ) $(ICEOBJ)\
		 $(TxtOBJ) $(xkbfileOBJ) $(XiOBJ) $(XdmcpOBJ) \
		 $(XextOBJ) $(XIEOBJ)

clean:
	rm *.o *.a

install: all
	cp libX11.a /usr/local/internetc/lib
 